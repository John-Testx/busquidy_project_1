{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@routes/*": ["routes/*"],
      "@api/*": ["api/*"],
      "@utils/*": ["utils/*"],
      "@common/*": ["common/*"],
      "@hooks/*": ["hooks/*"],
      "@contexts/*": ["contexts/*"]
    }
  },
  "include": ["src"]
}
